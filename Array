//Array

#include<iostream>
using namespace std;

int main(){
	int arr[] = {4,2,1,3,6};
	for(int i=0;i<5;i++){
		cout << *(arr+i) << endl;
	}
return 0;
}
                                                                    
//Array 2D 6x6 matrix

#include<iostream>
using namespace std;

int main(){
	int size1=6;
	 int *arr2=new int [size1];
	delete[]arr2;
	int ** arr3 =new int * [size1];
	delete[]arr3;
	int arr[] = {4,2,1,3,6};
	int size = sizeof(arr)/ sizeof(arr[0]);

	for(int i=0;i<size;++i){
		cout << *(arr+i)<< endl;
		//cout << *(arr+)i<< endl;
	}
return 0;
}
